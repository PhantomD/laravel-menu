{
    "name": "tasmir/laravel-menus",
    "description": "Laravel Menu management",
    "keywords": [
        "laravel",
        "menus",
        "navigation",
        "nav",
        "bootstrap"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.21|^10.0",
        "illuminate/config": "self.version",
        "illuminate/view": "self.version",
        "laravelcollective/html": "v6.4.1"
    },
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com"
        },
        {
            "name": "Pingpong Labs",
            "email": "pingpong.labs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tasmir\\Menus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tasmir\\Menus\\Tests\\": "Tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "~1.5.1",
        "orchestra/testbench": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.21.1"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Tasmir\\Menus\\MenusServiceProvider"
            ],
            "aliases": {
                "Menu": "Tasmir\\Menus\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "stable"
}
